<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="560.0" prefWidth="840.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.groupnine.banku.controllers.AccountsController">
   <children>
      <TabPane fx:id="tabPane" layoutX="-1.0" layoutY="-1.0" onMouseClicked="#tabPaneOnClick" prefHeight="560.0" prefWidth="840.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="tabParticulares" text="Particulares">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="particularAccountsTable" layoutX="5.0" prefHeight="537.0" prefWidth="840.0">
                           <columns>
                              <TableColumn prefWidth="75.0" text="Numero" />
                              <TableColumn prefWidth="193.0" text="Nome" />
                              <TableColumn prefWidth="303.0" text="Dono" />
                              <TableColumn minWidth="0.0" prefWidth="156.0" text="Balanco Atual" />
                              <TableColumn prefWidth="89.0" text="#Cartoes" />
                           </columns>
                        </TableView>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab fx:id="tabCorporativas" text="Corporativas">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="530.0" prefWidth="832.0">
                     <children>
                        <TableView fx:id="corporativeAccountsTable" layoutX="5.0" prefHeight="537.0" prefWidth="840.0">
                          <columns>
                            <TableColumn prefWidth="75.0" text="Numero" />
                            <TableColumn prefWidth="228.0" text="Dono" />
                              <TableColumn minWidth="0.0" prefWidth="188.0" text="Admin" />
                              <TableColumn prefWidth="143.0" text="Balanco Atual" />
                              <TableColumn prefWidth="184.0" text="#Usuarios Autorizados" />
                          </columns>
                        </TableView>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
            <Tab fx:id="tabTemporarias" text="Temporarias">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="temporaryAccountsTable" layoutX="5.0" prefHeight="537.0" prefWidth="840.0">
                           <columns>
                              <TableColumn prefWidth="75.0" text="Numero" />
                              <TableColumn prefWidth="152.0" text="Nome" />
                              <TableColumn prefWidth="208.0" text="Dono" />
                              <TableColumn minWidth="0.0" prefWidth="132.0" text="Balanco Atual" />
                              <TableColumn prefWidth="116.0" text="Criado em" />
                              <TableColumn prefWidth="134.0" text="Expira em" />
                           </columns>
                        </TableView>
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
        </tabs>
      </TabPane>
      <HBox alignment="BOTTOM_LEFT" layoutX="8.0" layoutY="523.0" prefHeight="33.0" prefWidth="826.0" spacing="270.0">
         <children>
            <Button fx:id="dashboardBtn" mnemonicParsing="false" onMouseClicked="#dashboardBtnOnClick" text="Voltar ao Dashboard" />
            <HBox alignment="BOTTOM_RIGHT" prefHeight="6.0" prefWidth="403.0">
               <children>
                  <Button mnemonicParsing="false" text="Criar Novo" />
                  <Button fx:id="removeBtn" mnemonicParsing="false" onMouseClicked="#removeBtnOnClick" text="Remover" />
               </children>
            </HBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
